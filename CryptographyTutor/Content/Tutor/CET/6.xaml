<UserControl x:Class="CryptographyTutor.Content.Tutor.CET._6"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <ScrollViewer>
            <StackPanel MinWidth="200">
                <TextBlock Text="Cryptography" Style="{StaticResource Heading1}" />
                <TextBlock Text="• can characterize cryptographic system by:"/>
<TextBlock Text="– type of encryption operations used"/>
<TextBlock Text="• substitution"/>
<TextBlock Text="• transposition"/>
<TextBlock Text="• product"/>
<TextBlock Text="– number of keys used"/>
<TextBlock Text="• single-key or private"/>
<TextBlock Text="• two-key or public"/>
<TextBlock Text="– way in which plaintext is processed"/>
<TextBlock Text="• block"/>
<TextBlock Text="• stream"/>

            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
