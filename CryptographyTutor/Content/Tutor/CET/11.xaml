<UserControl x:Class="CryptographyTutor.Content.Tutor.CET._11"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <ScrollViewer>
            <StackPanel MinWidth="200">
                <TextBlock Text="Classical Substitution Ciphers" Style="{StaticResource Heading1}" />
                <TextBlock Text="Content goes here" />
				<TextBlock Text="where letters of plaintext are replaced by" />
<TextBlock Text="other letters or by numbers or symbols" />
<TextBlock Text="• or if plaintext is viewed as a sequence of bits," />
<TextBlock Text="then substitution involves replacing plaintext" />
<TextBlock Text="bit patterns with ciphertext bit patterns" />

            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
