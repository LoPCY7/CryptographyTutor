<Window x:Class="CryptographyTutor.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CryptographyTutor"
        mc:Ignorable="d"
        Title="GameWindow" Height="207.563" Width="400">
    <Grid>
        <Label x:Name="label" Content="Select level:" HorizontalAlignment="Left" Height="29" Margin="22,18,0,0" VerticalAlignment="Top" Width="198"/>
        <Button x:Name="button" Content="Level 1" HorizontalAlignment="Left" Height="56" Margin="52,52,0,0" VerticalAlignment="Top" Width="111" Click="button_Click"/>
        <Button x:Name="button1" Content="Level 2" HorizontalAlignment="Left" Height="56" Margin="232,52,0,0" VerticalAlignment="Top" Width="111" IsEnabled="False"/>
        <Button x:Name="button2" Content="Exit" HorizontalAlignment="Left" Height="24" Margin="143,130,0,0" VerticalAlignment="Top" Width="93" Click="button2_Click"/>

    </Grid>
</Window>
